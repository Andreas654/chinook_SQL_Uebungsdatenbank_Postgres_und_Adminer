{
  "name": "PostgreSQL and Adminer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "adminer",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "forwardPorts": [
    8080,
    5432
  ],
  "postCreateCommand": "sleep 5 && docker-compose up -d"
}
